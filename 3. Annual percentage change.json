{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Annual Growth in Malaysia",
  "width": 600,
  "height": 400,
  "data": {
    "url": "data/Annual percentage change.csv"
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "scale": {
        "domain": ["2000-01-01", "2020-12-31"]
      },
      "axis": {
        "title": "Year"
      }
    },
    "y": {
      "field": "Annual growth",
      "type": "quantitative",
      "scale": {
        "type": "linear",
        "domain": [-20, 20]
      }
    },
    "color": {
      "field": "Entity",
      "type": "nominal",
      "legend": {
        "title": "Country"
      }
    }
  }
}
